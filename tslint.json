{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "trailing-comma": false,
        "semicolon": {
            "options": "always"
        },
        "interface-name": false,
        "curly": false,
        "object-literal-sort-keys": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
