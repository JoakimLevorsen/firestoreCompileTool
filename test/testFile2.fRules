interface A {
    foo: Number;
    bar: String | Timestamp;
}

interface B {
    value: Bool;
}

match /collection/[item] {
    read, delete: {
        if item.a == true {
            return item is B;
        }
        if item is B {
            return true
        }
        return item is A;
    },
    write: request.resource is A;
}